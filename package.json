{
  "name": "koapi-boilerplate",
  "version": "0.1.0",
  "description": "RESTful API framework based on koajs",
  "main": "app.js",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "scripts": {
    "build": "babel --presets es2015,stage-0 -d build/ src/",
    "dev"    : "nodemon -L -x ./node_modules/.bin/babel-node src/app.js",
    "start"  : "npm run build && node build/app.js",
    "knex"   : "knex"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garbin/koapi-boilerplate.git"
  },
  "keywords": [
    "RESTful",
    "API",
    "Framework",
    "koa"
  ],
  "author": "Garbin Huang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/garbin/koapi-boilerplate/issues"
  },
  "homepage": "https://github.com/garbin/koapi-boilerplate#readme",
  "dependencies": {
    "koapi": "*",
    "mysql": "^2.10.2"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "nodemon": "^1.9.1"
  }
}
