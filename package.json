{
  "name": "koapi-boilerplate",
  "version": "0.2.0",
  "description": "RESTful API framework based on koajs",
  "main": "koapi.js",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.test.js",
      "!test/__lib__/**/*"
    ],
    "source": [
      "**/*.{js,jsx,es}",
      "!build/**/*"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "extension": [
      ".jsx",
      ".es"
    ]
  },
  "scripts": {
    "start": "node --harmony run.js server",
    "service": "node --harmony run.js service",
    "watch": "nodemon -x \"node --harmony\" -e js,es,jsx -L run.js --",
    "build": "node --harmony run.js build",
    "test": "node --harmony run.js test",
    "migrate": "node --harmony run.js migrate",
    "docs": "http-server ./docs -p 5003"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garbin/koapi-boilerplate.git"
  },
  "keywords": [
    "RESTful",
    "API",
    "Framework",
    "koa"
  ],
  "author": "Garbin Huang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/garbin/koapi-boilerplate/issues"
  },
  "homepage": "https://github.com/garbin/koapi-boilerplate#readme",
  "dependencies": {
    "axios": "^0.13.1",
    "blueimp-md5": "^2.3.0",
    "bull": "^1.0.0-rc1",
    "bull-ui": "^1.2.1",
    "commander": "^2.9.0",
    "config": "^1.20.2",
    "es6-promisify": "^4.1.0",
    "glob": "^7.0.6",
    "http-server": "^0.9.0",
    "joi": "^9.0.4",
    "koa-compose": "^3.1.0",
    "koa-oauth2orize": "^0.1.3",
    "koa-passport": "^2.2.1",
    "koapi": "^0.2.61",
    "lodash": "^4.12.0",
    "moment": "^2.13.0",
    "mysql": "^2.10.2",
    "node-resque": "^3.0.0",
    "node-schedule": "^1.1.1",
    "node-uuid": "^1.4.7",
    "oauth2orize-koa": "^1.3.2",
    "passport-github": "^1.1.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-oauth2": "^1.3.0",
    "passport-oauth2-client-password": "^0.1.2",
    "pg": "^6.0.3",
    "shelljs": "^0.7.3",
    "throng": "^4.0.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "apidoc": "^0.16.1",
    "apidoc-plugin-schema": "0.0.6",
    "babel-cli": "^6.9.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "fs-extra": "^0.30.0",
    "generate-schema": "^2.1.1",
    "nock": "^8.0.0",
    "nodemon": "^1.9.2",
    "nyc": "^7.1.0"
  }
}
